namespace HTL\SGMLStream\_Private;

function hhvm_may_want_this_method(): nothing {
  throw new \Error(
    'This operation is not supported. '.
    'This method was not deleted, because it might be used by the hhvm '.
    'internals. If HHVM called this method internally, please file a bug '.
    'report against this repository.',
  );
}
